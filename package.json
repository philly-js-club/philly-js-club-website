{
	"name": "philly-js-club-site",
	"version": "0.0.0",
	"description": "Website for the Philly JS Club. ðŸ¥³",
	"license": "MIT",
	"sideEffects": false,
	"scripts": {
		"build": "remix build",
		"dev": "remix dev",
		"format": "prettier \"**/*\" --ignore-unknown",
		"format:write": "pnpm format --write",
		"lint": "eslint . --max-warnings 0 --report-unused-disable-directives",
		"lint:knip": "knip",
		"lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
		"lint:package": "npmPkgJsonLint .",
		"lint:spelling": "cspell \"**\" \".github/**/*\"",
		"lint:styles": "stylelint \"**/*.css\"",
		"prepare": "husky install",
		"test": "playwright test --headed",
		"test:ci": "playwright test --project=chromium",
		"typecheck": "tsc"
	},
	"lint-staged": {
		"*": "prettier --ignore-unknown --write"
	},
	"resolutions": {
		"@typescript-eslint/eslint-plugin": "6.0.0-alpha.124",
		"@typescript-eslint/parser": "6.0.0-alpha.124"
	},
	"dependencies": {
		"@axe-core/playwright": "^4.6.1",
		"@playwright/test": "^1.32.3",
		"@remix-run/node": "^1.15.0",
		"@remix-run/react": "^1.15.0",
		"clsx": "^1.2.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"isbot": "latest",
		"playwright": "^1.32.3",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.15.0",
		"@remix-run/eslint-config": "^1.15.0",
		"@remix-run/serve": "^1.15.0",
		"@types/react": "^18.0.25",
		"@types/react-dom": "^18.0.8",
		"@typescript-eslint/eslint-plugin": "6.0.0-alpha.124",
		"@typescript-eslint/parser": "6.0.0-alpha.124",
		"cspell": "^6.31.1",
		"eslint": "^8.27.0",
		"eslint-plugin-deprecation": "^1.4.1",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-jsonc": "^2.7.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-regexp": "^1.14.0",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-typescript-sort-keys": "^2.3.0",
		"eslint-plugin-yml": "^1.5.0",
		"husky": "^8.0.3",
		"jsonc-eslint-parser": "^2.2.0",
		"knip": "^2.9.0",
		"lint-staged": "^13.2.1",
		"markdownlint": "^0.28.1",
		"markdownlint-cli": "^0.33.0",
		"npm-package-json-lint": "^6.4.0",
		"npm-package-json-lint-config-default": "^5.0.0",
		"prettier": "^2.8.7",
		"prettier-plugin-packagejson": "^2.4.3",
		"sentences-per-line": "^0.2.1",
		"stylelint": "^15.4.0",
		"stylelint-config-standard": "^32.0.0",
		"stylelint-order": "^6.0.3",
		"typescript": "^5.0.4",
		"yaml-eslint-parser": "^1.2.0"
	},
	"packageManager": "pnpm@8.2.0",
	"engines": {
		"node": ">=14"
	}
}
